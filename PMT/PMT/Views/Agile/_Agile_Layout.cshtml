<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>@ViewData["Title"] - PMT</title>
  <link rel="icon" href="/icons/PMT_favicon.png">
  <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/newStory.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/myStories.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/storyDetails.css" asp-append-version="true" />
  <script type="text/javascript" src="/lib/jquery/dist/jquery.js"></script>
  <script defer type="text/javascript" src="/js/site.js"></script>
</head>
<body>
  <div class="no-js-warning">This application relies heavily on javascript - please ensure it's enabled in your browser and refresh the page.</div>
  <script>$(".no-js-warning").remove();</script>

  <partial name="../_NavBar.cshtml">

  <main role="main">
      @RenderBody()
  </main>
</body>
</html>
